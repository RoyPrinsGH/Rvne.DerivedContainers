<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>Rvne.DerivedContainers</PackageId>
    <Version>0.1.1</Version>
    <Authors>Roy Prins (Rvne)</Authors>
    <Description>Derive a tree of nested instaadsdasadsasdnces of the same type within a given instance, and perform broad operations on it</Description>
    <RepositoryUrl>https://github.com/RoyPrinsGH/Rvne.DerivedContainers</RepositoryUrl>
    <PackageTags>options;attributes;reflection</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFrameworks>net8.0;net10.0</TargetFrameworks>
    <LangVersion>14.0</LangVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md" Pack="true" PackagePath="\" />
  </ItemGroup>

</Project>
